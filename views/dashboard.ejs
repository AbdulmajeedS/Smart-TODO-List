<!DOCTYPE html>
<html lang="en">
  <%- include('partials/_header.ejs') %>
<body>
  <%- include('partials/_navbar.ejs') %>
  <div id="container">
    <h1> Smart-To-Do <i class="fa fa-plus"></i></h1>
    <h2>Catagories</h2>
    <p>Select catagories from drop-down list:</p>
    <form action="/login">
      <label for="Catagories">Choose catagories:</label>
      <select id="cat" name="cars">
        <option value="watch">To Watch</option>
        <option value="eat">To Eat</option>
        <option value="buy">To Buy</option>
        <option value="read">To Read</option>
      </select>

    </form>

    <input type="text" placeholder="Let's Add Something Amazing TO-Do!">
    <ul>
      <% for(const item of items ) { %>
        <li><span><i class="fa fa-trash"></i></span> <%=item.title%> </li>
        <% } %>

    </ul>
  </div>
</body>

</html>
